{
   "name": "boids-wasm",
   "version": "1.0.0",
   "main": "index.html",
   "scripts": {
      "test": "echo \"Error: no test specified\" && exit 1",
      "build:css": "tailwindcss -i ./styles.css -o ./dist/tailwind.css --minify",
      "watch:css": "tailwindcss -i ./assets/styles.css -o ./dist/tailwind.css --watch",
      "install:rust": "curl --proto '=https' --tlsv1.2 -sSf https://sh.rustup.rs | sh -s -- -y && . $HOME/.cargo/env && cargo install --locked trunk",
      "build:project": "npm run build:css && trunk build --release"
   },
   "keywords": [],
   "author": "",
   "license": "ISC",
   "description": "Boids simulation with WebAssembly and TailwindCSS",
   "dependencies": {
      "tailwindcss": "^3.4.10"
   },
   "devDependencies": {
      "daisyui": "^4.12.10"
   }
}
